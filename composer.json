{
    "name": "kraziegent/laraform",
    "description": "A Dynamic form generator for Laravel using blade.",
    "keywords": [
        "laravel",
        "html form",
        "blade",
        "bootstrap",
        "tailwind"
    ],
    "homepage": "https://github.com/kraziegent/laraform",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Olumolu",
            "email": "olumolupeter@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^7.5|^8.4|^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Kraziegent\\Laraform\\": "src",
            "Kraziegent\\Laraform\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kraziegent\\Laraform\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kraziegent\\Laraform\\LaraformServiceProvider"
            ],
            "aliases": {
                "Laraform": "Kraziegent\\Laraform\\LaraformFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spatie"
        },
        {
            "type": "other",
            "url": "https://spatie.be/open-source/support-us"
        }
    ]
}
